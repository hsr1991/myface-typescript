<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Users</h1>

    <% results.forEach(function(user) {%>
            <p><%= user.username %></p>
            <%= user.name %>
            <p><%= user.id %></p>
            <p><img src="<%=user.profileImageUrl%>"></p>
        <% }); %>

<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>
<%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
<% } %>
</body>
<%- include('_footer') -%>
</html>